generator client {
  provider = "prisma-client-js"
  output   = "generated"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Integration {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  name      String
  phone     String   @unique
}

model Contact {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  phone     String   @unique
  image     String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
